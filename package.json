    "src/**/*.{js,jsx,json}": [
      "yarn lint:js",
      "git add"
    ],
    "src/**/*.scss": [
      "yarn lint:style",
      "git add"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "local-cms": "npx netlify-cms-proxy-server",
    "clean": "gatsby clean",
    "develop": "gatsby develop --open",
    "format": "prettier --write ',**/*.+(js|json|md)'",
    "lint": "yarn lint:js & yarn lint:style",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:style": "stylelint src/**/*.scss",
    "lint:prettier": "prettier --check '**/*.+(js|jsx|json|css|scss)'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
